<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xerces="http://xerces.apache.org"
	xmlns:xades="http://uri.etsi.org/01903/v1.3.2#" xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
	xmlns:earsiv="http://earsiv.efatura.gov.tr" targetNamespace="http://earsiv.efatura.gov.tr"
	xmlns:xades141="http://uri.etsi.org/01903/v1.4.1#" elementFormDefault="qualified"
	attributeFormDefault="unqualified" xmlns:ns1="http://zraporu.efatura.gov.tr">
	<xs:import namespace="http://uri.etsi.org/01903/v1.3.2#" schemaLocation="XAdES.xsd"/>
	<xs:import namespace="http://uri.etsi.org/01903/v1.4.1#" schemaLocation="XAdESv141.xsd"/>
	<xs:import namespace="http://www.w3.org/2000/09/xmldsig#"
		schemaLocation="xmldsig-core-schema.xsd"/>
	<xs:element name="eArsivRaporu">
		<xs:annotation>
			<xs:documentation>eArsiv Rapor Bilgileri.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="baslik" type="earsiv:baslikType" maxOccurs="1" minOccurs="1"/>
				<xs:choice minOccurs="1" maxOccurs="unbounded">
					<xs:element name="fatura" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="faturaNo" type="earsiv:idType"/>
								<xs:element name="faturaUUID" type="earsiv:uuidType"/>
								<xs:element name="faturaTip" type="earsiv:faturaTipEnum"/>
								<xs:element name="ytbBilgileri" minOccurs="0" maxOccurs="1">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="ytbNo" minOccurs="1">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:pattern value="[0-9]{6}"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="tarih" type="xs:date" minOccurs="1"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="gonderimSekli">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="KAGIT"/>
											<xs:enumeration value="ELEKTRONIK"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="dosyaAdi" type="xs:string"/>
								<xs:element name="ozetDeger" type="xs:string"/>
								<xs:element name="duzenlenmeTarihi" type="xs:date"/>
								<xs:element name="duzenlenmeZamani" type="earsiv:timeType"/>
								<xs:element name="toplamTutar" type="earsiv:currencyValueType"/>
								<xs:element name="toplamIskonto" type="earsiv:currencyValueType"/>
								<xs:element name="odenecekTutar" type="earsiv:currencyValueType"/>
								<xs:element name="paraBirimi" type="earsiv:currencyCode"/>
								<xs:element name="dovizKuru" minOccurs="1">
									<xs:simpleType>
										<xs:restriction base="xs:decimal">
											<xs:totalDigits value="18"/>
											<xs:fractionDigits value="6"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="faturaUrl" minOccurs="1" type="earsiv:urlType"/>
								<xs:element name="vergiBilgisi" type="earsiv:vergiBilgisiType"/>
								<xs:element name="aliciBilgileri" type="earsiv:aliciType"/>
								<xs:element name="internetSatisBilgi" minOccurs="0" maxOccurs="1">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="webAdresi" maxOccurs="1">
												<xs:simpleType>
												<xs:restriction base="xs:string">
												<xs:minLength value="2"/>
												</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="odemeSekli" type="earsiv:odemeTip"/>
											<xs:element name="odemeAracisiAdi" type="xs:string"
												minOccurs="0"/>
											<xs:element name="odemeTarihi" type="xs:date"
												minOccurs="0"/>
											<xs:element name="gonderiBilgileri" minOccurs="1">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="gonderimTarihi" type="xs:date"/>
												<xs:element name="gonderiTasiyan"
												type="earsiv:kisiType"/>
												</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="ynOkcFisBilgisi" minOccurs="0"
									maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="okcSeriNo" type="xs:string"/>
											<xs:element name="zNo" type="xs:string"/>
											<xs:element name="fisNo" type="xs:string"/>
											<xs:element name="fisTip" type="earsiv:fisTipType"/>
											<xs:element name="fisTarih" type="xs:date"/>
											<xs:element name="fisZaman" type="earsiv:timeType"
												minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="sarjAnlik" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="sarjZamani"
												type="earsiv:sarjZamanType"/>
											<xs:element name="plaka" type="xs:string"/>
											<xs:element name="toplamTutar"
												type="earsiv:currencyValueType"/>
											<xs:element name="indirim" type="xs:string"
												minOccurs="0"/>
											<xs:element name="kalemDetay"
												type="earsiv:kalemDetayType"/>
											<xs:element name="esuSeriNo" type="xs:string"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="sarj" minOccurs="0" maxOccurs="1">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="sarjZamani"
												type="earsiv:sarjZamanType"/>
											<xs:element name="esuRaporID" type="earsiv:uuidType"/>
											<xs:element name="plaka" type="xs:string"/>
											<xs:element name="toplamTutar"
												type="earsiv:currencyValueType"/>
											<xs:element name="indirim" type="xs:string"
												minOccurs="0"/>
											<xs:element name="kalemDetay"
												type="earsiv:kalemDetayType" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="imzaZamani" type="xs:dateTime" minOccurs="1"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="faturaIptal" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="faturaNo" type="earsiv:idType"/>
								<xs:element name="iptalTarihi" type="xs:date"/>
								<xs:element name="toplamTutar" type="earsiv:currencyValueType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="faturaItiraz" minOccurs="0" maxOccurs="unbounded"
						type="earsiv:belgeItirazType"/>
					<xs:element name="mustahsilMakbuz" maxOccurs="unbounded" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="makbuzNo" type="earsiv:idType"/>
								<xs:element name="dosyaAdi" type="xs:string"/>
								<xs:element name="ozetDeger" type="xs:string"/>
								<xs:element name="duzenlenmeTarihi" type="xs:date"/>
								<xs:element name="duzenlenmeZamani" type="earsiv:timeType"/>
								<xs:element name="toplamTutar" type="earsiv:currencyValueType"/>
								<xs:element name="odenecekTutar" type="earsiv:currencyValueType"/>
								<xs:element name="paraBirimi" type="earsiv:currencyCode"/>
								<xs:element name="mustahsilMakbuzUrl" minOccurs="1"
									type="earsiv:urlType"/>
								<xs:element name="vergiBilgisi" type="earsiv:vergiBilgisiType"/>
								<xs:element name="mustahsilBilgileri" type="earsiv:aliciTypeMin"/>
								<xs:element name="imzaZamani" type="xs:dateTime" minOccurs="1"/>
								<xs:element name="ynOkcFisBilgisi" minOccurs="0"
									maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="okcSeriNo" type="xs:string"/>
											<xs:element name="zNo" type="xs:string"/>
											<xs:element name="fisNo" type="xs:string"/>
											<xs:element name="fisTip" type="earsiv:fisTipType"/>
											<xs:element name="fisTarih" type="xs:date"/>
											<xs:element name="fisZaman" type="earsiv:timeType"
												minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="mustahsilMakbuzIptal" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="makbuzNo" type="earsiv:idType"/>
								<xs:element name="iptalTarihi" type="xs:date"/>
								<xs:element name="toplamTutar" type="earsiv:currencyValueType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="serbestMeslekMakbuz" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="makbuzNo" type="earsiv:idType"/>
								<xs:element name="gonderimSekli">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="KAGIT"/>
											<xs:enumeration value="ELEKTRONIK"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="dosyaAdi" type="xs:string"/>
								<xs:element name="ozetDeger" type="xs:string"/>
								<xs:element name="duzenlenmeTarihi" type="xs:date"/>
								<xs:element name="duzenlenmeZamani" type="earsiv:timeType"
									minOccurs="0"/>
								<xs:element name="toplamTutar" type="earsiv:currencyValueType"/>
								<xs:element name="odenecekTutar" type="earsiv:currencyValueType"/>
								<xs:element name="paraBirimi" type="earsiv:currencyCode"/>
								<xs:element name="dovizKuru" minOccurs="1">
									<xs:simpleType>
										<xs:restriction base="xs:decimal">
											<xs:totalDigits value="18"/>
											<xs:fractionDigits value="6"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="serbestMeslekMakbuzUrl" minOccurs="1"
									type="earsiv:urlType"/>
								<xs:element name="vergiBilgisi" type="earsiv:vergiBilgisiType"/>
								<xs:element name="aliciBilgileri" type="earsiv:aliciTypeMin"/>
								<xs:element name="imzaZamani" type="xs:dateTime" minOccurs="1"/>
								<xs:element name="ynOkcFisBilgisi" minOccurs="0"
									maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="okcSeriNo" type="xs:string"/>
											<xs:element name="zNo" type="xs:string"/>
											<xs:element name="fisNo" type="xs:string"/>
											<xs:element name="fisTip" type="earsiv:fisTipType"/>
											<xs:element name="fisTarih" type="xs:date"/>
											<xs:element name="fisZaman" type="earsiv:timeType"
												minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="serbestMeslekMakbuzIptal" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="makbuzNo" type="earsiv:idType"/>
								<xs:element name="iptalTarihi" type="xs:date"/>
								<xs:element name="toplamTutar" type="earsiv:currencyValueType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="serbestMeslekMakbuzItiraz" minOccurs="0" maxOccurs="unbounded"
						type="earsiv:belgeItirazType"/>
					<xs:element name="mRapor" maxOccurs="unbounded" minOccurs="0"
						type="earsiv:mRaporType"/>
					<xs:element name="ymRapor" maxOccurs="unbounded" minOccurs="0"
						type="earsiv:ymRaporType"/>
					<xs:element name="bankReceipt" maxOccurs="unbounded" minOccurs="0"
						type="earsiv:bankReceiptType"/>
					<xs:element name="bankReceiptIptal" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="dekontNo" type="earsiv:idType"/>
								<xs:element name="UUID" type="earsiv:uuidType"/>
								<xs:element name="iptalTarihi" type="xs:date"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="adisyon" maxOccurs="unbounded" minOccurs="0"
						type="earsiv:eCheckType"/>
					<xs:element name="adisyonIptal" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="adisyonNo" type="earsiv:idType"/>
								<xs:element name="UUID" type="earsiv:uuidType"/>
								<xs:element name="iptalTarihi" type="xs:date"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
		<xs:unique name="faturaNo">
			<xs:selector xpath="earsiv:fatura"/>
			<xs:field xpath="earsiv:faturaNo"/>
		</xs:unique>
		<xs:unique name="faturaIptalNo">
			<xs:selector xpath="earsiv:faturaIptal"/>
			<xs:field xpath="earsiv:faturaNo"/>
		</xs:unique>
		<xs:unique name="faturaItirazNo">
			<xs:selector xpath="earsiv:faturaItiraz"/>
			<xs:field xpath="earsiv:belgeNo"/>
		</xs:unique>
		<xs:unique name="serbestMeslekMakbuzNo">
			<xs:selector xpath="earsiv:serbestMeslekMakbuz"/>
			<xs:field xpath="earsiv:makbuzNo"/>
		</xs:unique>
		<xs:unique name="serbestMeslekMakbuzIptalNo">
			<xs:selector xpath="earsiv:serbestMeslekMakbuzIptal"/>
			<xs:field xpath="earsiv:makbuzNo"/>
		</xs:unique>
		<xs:unique name="serbestMeslekMakbuzItirazNo">
			<xs:selector xpath="earsiv:serbestMeslekMakbuzItiraz"/>
			<xs:field xpath="earsiv:belgeNo"/>
		</xs:unique>
		<xs:unique name="mustahsilMakbuzNo">
			<xs:selector xpath="earsiv:mustahsilMakbuz"/>
			<xs:field xpath="earsiv:makbuzNo"/>
		</xs:unique>
		<xs:unique name="mustahsilMakbuzIptalNo">
			<xs:selector xpath="earsiv:mustahsilMakbuzIptal"/>
			<xs:field xpath="earsiv:makbuzNo"/>
		</xs:unique>
		<xs:unique name="mustahsilMakbuzItirazNo">
			<xs:selector xpath="earsiv:mustahsilMakbuzItiraz"/>
			<xs:field xpath="earsiv:belgeNo"/>
		</xs:unique>
		<xs:unique name="adisyonNo">
			<xs:selector xpath="earsiv:adisyon"/>
			<xs:field xpath="earsiv:adisyonNo"/>
		</xs:unique>
		<xs:unique name="adisyonIptalNo">
			<xs:selector xpath="earsiv:adisyonIptal"/>
			<xs:field xpath="earsiv:adisyonNo"/>
		</xs:unique>
		<xs:unique name="adisyonUUID">
			<xs:selector xpath="earsiv:adisyon"/>
			<xs:field xpath="earsiv:UUID"/>
		</xs:unique>
		<xs:unique name="adisyonIptalUUID">
			<xs:selector xpath="earsiv:adisyonIptal"/>
			<xs:field xpath="earsiv:UUID"/>
		</xs:unique>
		<xs:unique name="dekontNo">
			<xs:selector xpath="earsiv:bankReceipt"/>
			<xs:field xpath="earsiv:dekontNo"/>
		</xs:unique>
		<xs:unique name="dekontIptalNo">
			<xs:selector xpath="earsiv:bankReceipt"/>
			<xs:field xpath="earsiv:dekontNo"/>
		</xs:unique>
		<xs:unique name="dekontUUID">
			<xs:selector xpath="earsiv:bankReceipt"/>
			<xs:field xpath="earsiv:UUID"/>
		</xs:unique>
		<xs:unique name="dekontIptalUUID">
			<xs:selector xpath="earsiv:bankReceipt"/>
			<xs:field xpath="earsiv:UUID"/>
		</xs:unique>
	</xs:element>
	<xs:complexType name="baslikType">
		<xs:sequence>
			<xs:element name="versiyon" type="xs:string"/>
			<xs:element name="mukellef" type="earsiv:vknTcknType"/>
			<xs:element name="hazirlayan" type="earsiv:vknTcknType"/>
			<xs:element name="raporNo" type="earsiv:uuidType"/>
			<xs:element name="donemBaslangicTarihi" type="xs:date"/>
			<xs:element name="donemBitisTarihi" type="xs:date"/>
			<xs:element name="bolumBaslangicTarihi" type="xs:date"/>
			<xs:element name="bolumBitisTarihi" type="xs:date"/>
			<xs:element name="bolumNo" type="xs:int"/>
			<xs:element ref="ds:Signature"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="vknType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d\d\d\d\d\d\d\d\d\d"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tcknType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d\d\d\d\d\d\d\d\d\d\d"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="idType">
		<xs:restriction base="xs:string">
			<xs:length value="16"/>
			<xs:pattern value="[A-Za-z0-9]{3}20[0-9]{2}[0-9]{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="vknTcknType">
		<xs:choice>
			<xs:element name="vkn" type="earsiv:vknType"/>
			<xs:element name="tckn" type="earsiv:tcknType"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="kisiType">
		<xs:sequence>
			<xs:element name="tuzelKisi" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="vkn" type="earsiv:vknType"/>
						<xs:element name="unvan">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="gercekKisi" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="tckn" type="earsiv:tcknType"/>
						<xs:element name="adiSoyadi">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="aliciType">
		<xs:sequence>
			<xs:element name="tuzelKisi" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="vkn" type="earsiv:vknType"/>
						<xs:element name="unvan">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="gercekKisi" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="tckn" type="earsiv:tcknType"/>
						<xs:element name="adiSoyadi">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="tesisatNumarasi" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="aliciTypeMin">
		<xs:sequence>
			<xs:element name="tuzelKisi" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="vkn" type="earsiv:vknType"/>
						<xs:element name="unvan">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="gercekKisi" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="tckn" type="earsiv:tcknType"/>
						<xs:element name="adiSoyadi">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="odemeTipEnum">
		<xs:restriction base="xs:string">
			<xs:enumeration value="KREDIKARTI/BANKAKARTI"/>
			<xs:enumeration value="EFT/HAVALE"/>
			<xs:enumeration value="KAPIDAODEME"/>
			<xs:enumeration value="ODEMEARACISI"/>
